amc:ear_pointiness rdf:type rdfs:Datatype ;
    owl:onDatatype xsd:decimal ;
    owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) .

amc:item_type rdf:type rdfs:Datatype ;
    owl:oneOf ( 
        "sleeve" "shoe" "neckline" "pocket" "dress" "top-t-shirt-sweatshirt" "pants" "collar" 
        "bag-wallet" "zipper" "jacket" "belt" "skirt" "rivet" "lapel" "glasses" 
        "headband-head-covering-hair-accessory" "bead" "shorts" "coat" "shirt-blouse" 
        "tights-stockings" "sock" "watch" "ruffle" "hat" "buckle" "applique" "scarf" 
        "flower" "tassel" "hood" "glove" "fringe" "vest" "jumpsuit" "sweater" 
        "epaulette" "sequin" "cardigan" "leg-warmer" "ribbon" "bow" "cape" "umbrella" "tie"
    ) .

amc:color_palette rdf:type rdfs:Datatype ;
    owl:oneOf ( "preset-fantasy" "preset-pastel-cyber" "preset-traditional" "preset-techwear" "preset-monochrome" "custom" ) .

amc:pattern_theme rdf:type rdfs:Datatype ;
    owl:oneOf ( "traditional-geometric" "cyber-tech" "floral" "mystical-symbols" "cute-icons" "abstract-modern" "none" ) .

amc:thematic_embellishments rdf:type rdfs:Datatype ;
    owl:oneOf ( "fantasy" "cyberprep" "traditional" ) .

# Additional Properties
amc:poseType rdf:type owl:DatatypeProperty ;
    rdfs:domain amc:Pose ;
    rdfs:range xsd:string ;
    rdfs:comment "Type of pose, e.g., standing, sitting." ;
    owl:hasValue "A-pose" .

amc:handOrientation rdf:type owl:DatatypeProperty ;
    rdfs:domain amc:Pose ;
    rdfs:range xsd:string ;
    rdfs:comment "Orientation of hands, e.g., palms down, palms up." ;
    owl:hasValue "palms down" .

amc:backgroundColor rdf:type owl:DatatypeProperty ;
    rdfs:domain amc:BackgroundSetting ;
    rdfs:range xsd:string ;
    rdfs:comment "Color of the background, e.g., beige, blue." ;
    owl:hasValue "beige" .

amc:lightingStyle rdf:type owl:DatatypeProperty ;
    rdfs:domain amc:BackgroundSetting ;
    rdfs:range xsd:string ;
    rdfs:comment "Style of lighting, e.g., soft, hard." ;
    owl:hasValue "soft" .

amc:feminineMasculineStyle rdf:type owl:DatatypeProperty ;
    rdfs:domain amc:AvatarModelContext ;
    rdfs:range xsd:decimal ;
    rdfs:comment "A value between 0.0 (fully feminine) and 1.0 (fully masculine)." ;
    owl:hasValue 0.0 .
